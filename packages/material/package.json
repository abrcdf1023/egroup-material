{
  "name": "@e-group/material",
  "version": "3.38.0",
  "private": false,
  "author": "eGroup Team",
  "description": "Custom react components from @material-ui.",
  "keywords": [
    "eGroup",
    "react",
    "react-component",
    "material design",
    "material-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:eGroupAI/egroup-material.git",
    "directory": "packages/material"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eGroupAI/egroup-material/issues"
  },
  "homepage": "https://eGroupAI.github.io/egroup-material",
  "dependencies": {
    "@types/warning": "^3.0.0",
    "clsx": "^1.1.1",
    "warning": "^4.0.3"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf build",
    "build": "yarn clean && yarn build:esm && yarn build:tsd && yarn build:copy-files",
    "build:tsd": "tsc -p ../../tsconfig.build.json",
    "build:esm": "babel --config-file ../../babel.config.js ./src --out-dir ./build --extensions .js,.ts,.jsx,.tsx",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prepare": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.4.0",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "./index.js",
  "typings": "./index.d.ts"
