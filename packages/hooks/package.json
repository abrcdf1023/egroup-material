    "mobile-detect": "^1.4.3",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0",
    "react-redux": "^7.2.1",
    "redux-form": "^8.3.6",
    "swr": "^0.3.6"
  },
