{
  "name": "@e-group/material-module",
  "version": "3.37.13",
  "private": false,
  "author": "eGroup Team",
  "description": "EGroup Team react component modules.",
  "keywords": [
    "eGroup",
    "react",
    "react-component",
    "modules"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:eGroupAI/egroup-material.git",
    "directory": "packages/material-module"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eGroupAI/egroup-material/issues"
  },
  "homepage": "https://eGroupAI.github.io/egroup-material",
  "dependencies": {
    "@e-group/material": "^3.37.9",
    "@e-group/utils": "^6.24.0",
    "clsx": "^1.1.1"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf build",
    "build": "yarn clean && yarn build:esm && yarn build:tsd && yarn build:copy-files",
    "build:tsd": "tsc ./src/index.ts --declaration --jsx react --esModuleInterop --emitDeclarationOnly",
    "build:esm": "babel --config-file ../../babel.config.js ./src --out-dir ./build --extensions .js,.ts,.jsx,.tsx",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prepare": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "^3.0.13",
    "@types/recharts": "^1.8.14",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "recharts": "^1.8.5"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
