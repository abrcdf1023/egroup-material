{
  "name": "@e-group/material-lab",
  "version": "3.37.3",
  "private": false,
  "author": "eGroup Team",
  "description": "EGroup Team Lab - Incubator for EGroup Team experimental React components.",
  "keywords": [
    "eGroup",
    "react",
    "react-component",
    "lab"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:eGroupAI/egroup-material.git",
    "directory": "packages/material-lab"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eGroupAI/egroup-material/issues"
  },
  "homepage": "https://eGroupAI.github.io/egroup-material",
  "dependencies": {
    "@e-group/hooks": "^6.24.5",
    "@e-group/material": "^3.37.3",
    "clsx": "^1.1.1",
    "date-fns": "^2.14.0",
    "warning": "^4.0.3"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf build",
    "build": "yarn clean && yarn build:esm && yarn build:tsd && yarn build:copy-files",
    "build:tsd": "tsc ./src/index.ts --declaration --jsx react --esModuleInterop --emitDeclarationOnly",
    "build:esm": "babel --config-file ../../babel.config.js ./src --out-dir ./build --extensions .js,.ts,.jsx,.tsx",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prepare": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@e-group/ckeditor5-build-classic": "^21.0.1",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "draft-js": "^0.11.5",
    "highlight.js": "^10.1.2",
    "immutable": "^4.0.0-rc.12",
    "marked": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
