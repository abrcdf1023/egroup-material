{
  "name": "@e-group/material-lab",
  "version": "3.32.2",
  "private": false,
  "author": "eGroup Team",
  "description": "EGroup Team Lab - Incubator for EGroup Team experimental React components.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "keywords": [
    "eGroup",
    "react",
    "react-component",
    "lab"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:eGroupAI/egroup-material.git",
    "directory": "packages/material-lab"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eGroupAI/egroup-material/issues"
  },
  "homepage": "https://eGroupAI.github.io/egroup-material",
  "dependencies": {
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@e-group/ckeditor5-build-classic": "^18.0.3",
    "@e-group/hooks": "^6.22.4",
    "@e-group/material": "^3.32.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.14.0",
    "draft-js": "^0.11.5",
    "highlight.js": "^10.1.0",
    "marked": "^1.1.0",
    "warning": "^4.0.3"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf build",
    "build": "yarn clean && yarn build:esm && tsc ./src/index.ts --declaration --jsx react --esModuleInterop --emitDeclarationOnly && yarn build:copy-files",
    "build:esm": "babel --config-file ../../babel.config.js ./src --out-dir ./build --extensions .js,.ts,.jsx,.tsx",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prepare": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
