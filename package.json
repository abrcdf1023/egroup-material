{
  "name": "@e-group/react-material-components",
  "version": "1.2.14",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "immutable": "^4.0.0-rc.12",
    "mui-datatables": "^2.0.0-beta.58",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-immutable-proptypes": "^2.1.0",
    "react-intl": "^2.8.0",
    "react-select": "^2.4.2",
    "redux-form": "^8.1.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.2",
    "@storybook/addon-info": "^5.0.2",
    "@storybook/addon-links": "^5.0.2",
    "@storybook/addon-notes": "^5.0.2",
    "@storybook/addon-storysource": "^5.0.2",
    "@storybook/addons": "^5.0.2",
    "@storybook/react": "^5.0.2",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
    "eslint": "^5.15.2",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "^3.4.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "7.x",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable": "^4.0.0",
    "typescript": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"yarn storybook\" \"yarn dev\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir ./dist && yarn build:copy-files",
    "build:copy-files": "node ./scripts/copy-files.js",
    "dev": "babel ./src --out-dir ./dist --watch",
    "prepare": "yarn clean && yarn build"
  },
  "author": "JerryP <egroup.jerrylee@gmail.com>",
  "license": "MIT",
  "description": "eGroup team's react-material components"
}
